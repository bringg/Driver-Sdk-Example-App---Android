<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <stroke
                android:width="1dp"
                android:color="#B6B6B6" />
            <padding
                android:bottom="8dp"
                android:left="16dp"
                android:right="16dp"
                android:top="8dp" />
        </shape>
    </item>
</layer-list>